# Database (RDS PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:YOUR_PASSWORD@your-rds-endpoint.region.rds.amazonaws.com:5432/finance_manager

# JWT Authentication
JWT_SECRET=generate_a_64_character_random_string_here_for_security
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# AI Services (Anthropic Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Error Monitoring (optional)
SENTRY_DSN=

# CORS Origins (frontend URLs)
CORS_ORIGINS=["https://getkonta.app","https://www.getkonta.app"]

# Environment
ENVIRONMENT=production
DEBUG=false
